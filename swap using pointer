#include <stdio.h>
int main() {
    int *ptr,*ptr1,a,b,temp;
    printf("enter your values:- ");
    scanf("%d" "%d",&a,&b);
    printf("Before swapping a=%d,b=%d",a,b);
    ptr=&a;
    ptr1=&b;
    temp=*ptr;
    *ptr=*ptr1;
    *ptr1=temp;
    printf("After swapping a=%d,b=%d",a,b);
    return 0;
}
